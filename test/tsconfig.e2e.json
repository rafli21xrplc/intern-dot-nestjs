
{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "declaration": false,
    "noEmit": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2021",
    "sourceMap": true,
    "outDir": "../dist",
    "baseUrl": "./",
    "types": ["jest", "node"]
  },
  "include": ["**/*.e2e-spec.ts", "**/*.ts"]
}
